/* algoritmo insercion */


void ordInsercion(vector<int> arr)


{
    int n = arr.size();
    int key = 0;
    int j = 0;
    for(int i = 1; i < n; i++){
        key = arr[i];
        j = i - 1;

        while(j>=0 && arr[j] > key){
            arr[j+1] = arr[j];
            j--;
        }
        arr[j+1] = key;

    }

}


/* algoritmo insercion */
void ordInsercion(vector<int> arr)
{
    int i = 0;
    int j = 0;
    int aux = 0;

    i = 1;
    while (i < arr.size())
        {
        j = i;
        aux = arr[i];

        while (j > 0 && aux < arr[j-1])
                {
            arr[j] = arr[j - 1];
            j = j - 1;
        }

        arr[j] = aux;
        i = i + 1;
    }
}




